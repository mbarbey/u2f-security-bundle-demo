{% extends 'base.html.twig' %}

{% block body %}
<h1>{{ user.username }}</h1>
<table>
    <thead>
        <tr>
            <th>Key</th>
            <th>Counter</th>
        </tr>
    </thead>
    <tbody>
        {% for key in user.u2fKeys %}
            <tr>
                <td>{{ key.name }}</td>
                <td>{{ key.counter }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('user_list') }}">back</a>
{% endblock %}
